package synergyspace.gestures;

public class GestureEvent {

	protected long cursorID;
	public long getCursorID() {
		return cursorID;
	}

	public String getGestureClassName() {
		return gestureClassName;
	}

	protected String gestureClassName;
	protected float y;
	public float getY() {
		return y;
	}

	public float getX() {
		return x;
	}

	protected float x;

	public GestureEvent(long cursorID, String gestureClassName, float x, float y) {
		this.cursorID = cursorID;
		this.gestureClassName = gestureClassName;
		this.x = x;
		this.y = y;
	}

}
